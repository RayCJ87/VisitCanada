<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ut.travelproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Visit Canada"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".cityIntro"
            android:label="@string/title_activity_city_intro" />
        <activity
            android:name=".VancouverIntro"
            android:label="@string/title_activity_vancouver_intro" />
        <activity
            android:name=".CalgaryIntro"
            android:label="@string/title_activity_calgary_intro" />
        <activity
            android:name=".MontrealIntro"
            android:label="@string/title_activity_montreal_intro"></activity>
    </application>

</manifest>